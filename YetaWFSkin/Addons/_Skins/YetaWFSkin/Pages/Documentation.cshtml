@using global::YetaWF.Core.Pages
@using global::YetaWF.Core.Modules
@using global::YetaWF.Core.Menus
@using global::YetaWF.Core.Skins
@inherits global::YetaWF.Core.Pages.RazorPage
@{
    Manager.SetSkinOptions(UsingBootstrap: true, UsingBootstrapButtons:true, MinWidthForPopups: 970);
    //uncomment the following to use Modernizr with your skin (this is not required by the skin)
    //Manager.AddOnManager.AddAddOnGlobal("Modernizr.com", "Modernizr");
    // get the location of this skin's css files
    SkinDefinition skin = SkinDefinition.EvaluatedSkin(Manager.CurrentPage, Manager.IsInPopup);
    string cssUrl = Manager.GetCDNUrl(YetaWF.Core.Addons.VersionManager.FindSkinVersion(ref skin, Manager.IsInPopup).GetAddOnCssUrl());
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv='Content-Type' content='text/html; charset=UTF-8' />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    [[Manager,-MetatagsHtml]]
    [[Manager,-PageTitleHtml]]
    [[PageOrSite,-FavIconLink]]
    [[ThisPage,-CanonicalUrlLink]]
    [[ThisPage,-HrefLangHtml]]
</head>
<body class="@Manager.PageCss("pageSkinDocX")">
    <noscript><div class="yDivWarning" style="height:100px;text-align:center;vertical-align:middle">This site requires Javascript</div></noscript>

    <!-- Main Menu Navbar -->
    <nav id="navheader" class="navbar navbar-default navbar-fixed-top navbar-inverse hidden-print">
        <div class="navbar-header">
            <button id="tabToggle" type="button" class="navbar-toggle pull-left">
                <img alt="Index & Search" src="@(cssUrl)Icons/Browse.png">
            </button>
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".yPageMenu">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
        </div>
        @(Html.RenderModule<global::YetaWF.Modules.Menus.Modules.MainMenuModule>(m => {
            m.Name = "Main Menu Module (Skin)";
        }))
    </nav>

    @RenderPane("", "MainPane AnyPane")

    @(Html.RenderEditControl<global::YetaWF.Modules.PageEdit.Modules.EditModeModule>())
    @(Html.RenderPageControl<global::YetaWF.Modules.PageEdit.Modules.PageControlModule>())

    @(Html.RenderUniqueModuleAddOns())

    <script>
        if (typeof Softelvdm_Documentation_File !== 'undefined')
            Softelvdm_Documentation_File.initPage('#tabToggle', '#navheader', '.MainPane')
    </script>
</body>
</html>
